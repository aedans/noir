  "scripts": {
    "build-server": "tsc && copyfiles public/cards/*.cjs dist",
    "build-client": "vite build",
    "build": "npm run build-server & npm run build-client",
    "watch": "tsc --watch",
    "client": "vite",
    "server": "nodemon --delay 1 --watch \"./dist/**\" dist/server/index.js",
    "dev": "npm run build && concurrently --kill-others \"npm run watch\" \"npm run client\" \"npm run server\"",
    "gauntlet": "npm run build-server && cd dist && node gauntlet.js",
    "random": "npm run build-server && cd dist && node random.js",
    "random-sync": "npm run build-server && cd dist && node random.js --sync",
    "start": "npm run server",
    "format": "prettier --write \"**/*.{ts,js,cjs}\""
  },
